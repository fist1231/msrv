{
  "name": "@nress/users",
  "version": "1.0.0",
  "description": "nress users",
  "main": "users.js",
  "scripts": {
    "start": "nodemon --exec babel-node --presets=es2015 -- index.js",
    "dev": "set NODE_ENV=development && nodemon --exec babel-node --presets=es2015 -- index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-server": "mocha --exit --compilers js:babel-core/register ./tests/**/*.test.js",
    "babel-node": "babel-node --presets=es2015",
    "prebuild": "",
    "build": "babel . -d ./build -s && mkdir \"build/config\" && copy \"config/.\" \"build/config\"",
    "prestart": "npm build",
    "start-prod": "NODE_ENV=production node ./build/index.js"
  },
  "keywords": [
    "nress",
    "users"
  ],
  "author": "nress",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.13.3"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "mongoose": "^4.13.7",
    "rxjs": "^5.5.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.17.3"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "ignore": [
      "**/*.test.js",
      "node_modules/"
    ]
  }
}
